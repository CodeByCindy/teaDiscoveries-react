rom 'reactstrap';\nimport {Formik, Field, Form, ErrorMessage} from 'formik';\nimport { validateContactForm } from '../../utils/validateContactForm';\nconst ContactForm = () => {\n    const handleSubmit = (values, {resetForm}) => {\n        console.log('form values:', values);\n        console.log('in Jason format:', JSON.stringify(values));\n        resetForm();\n    };\n\n    return (\n        <Formik\n            initialValues={{\n                firstName: '',\n                lastName: '',\n                phoneNum: '',\n                email: '', \n                agree: false,\n                contactType: 'By Phone',\n                feedback: ''\n            }}\n            onSubmit={handleSubmit}\n            validate={validateContactForm}\n        >\n            <Form>\n                <FormGroup row>\n                    <Label htmlFor='firstName' md='2'>\n                        First Name\n                    </Label>\n                    <Col md='10'>\n                        <Field \n                            name = 'firstName'\n                            placeholder = 'First Name'\n                            className = 'form-control'\n                        />\n                        <ErrorMessage name='firstName'>\n                            {(msg) => <p className = 'text-danger'>{msg}</p>}\n                        </ErrorMessage>\n                    </Col>\n                </FormGroup>\n                <FormGroup row>\n                    <Label htmlFor='lastName' md='2'>\n                        Last Name\n                    </Label>\n                    <Col md='10'>\n                        <Field \n                            name = 'lastName'\n                            placeholder = 'Last Name'\n                            className = 'form-control'\n                        />\n                        <ErrorMessage name='lastName'>\n                            {(msg) => <p className = 'text-danger'>{msg}</p>}\n                        </ErrorMessage>\n                    </Col>\n                </FormGroup>\n                <FormGroup row>\n                    <Label htmlFor='phoneNum' md='2'>\n                        Phone Number\n                    </Label>\n                    <Col md='10'>\n                        <Field \n                            name = 'phoneNum'\n                            placeholder = 'Phone'\n                            className = 'form-control'\n                        />\n                        <ErrorMessage name='phoneNum'>\n                            {(msg) => <p className = 'text-danger'>{msg}</p>}\n                        </ErrorMessage>\n                    </Col>\n                </FormGroup>\n                <FormGroup row>\n                    <Label htmlFor='email' md='2'>\n                        Email\n                    </Label>\n                    <Col md='10'>\n                        <Field \n                            name = 'email'\n                            placeholder = 'Email'\n                            type = 'email'\n                            className = 'form-control'\n                        />\n                        <ErrorMessage name='email'>\n                            {(msg) => <p className = 'text-danger'>{msg}</p>}\n                        </ErrorMessage>\n                    </Col>\n                </FormGroup>\n                <FormGroup row>\n                    <Label check md={{size: 4, offset: 2}}>\n                        <Field \n                        name = 'agree'\n                        type = 'checkbox'\n                        className = 'form-check-input' \n                        /> {' '}\n\n                        May we contact you?\n                    </Label>\n                    <Col md='4'>\n                        <Field \n                            name = 'contactType'\n                            as = 'select'\n                            className = 'form-control'\n                        >\n                             <option>By Phone</option>\n                             <option>By Email</option>\n                        </Field>\n                    </Col>\n                </FormGroup>\n                <FormGroup row>\n                    <Label htmlFor='feedback' md='2'>\n                        Your Feedback\n                    </Label>\n                    <Col md='10'>\n                        <Field \n                            name = 'feedback'\n                            as = 'textarea'\n                            rows = '12'\n                            className = 'form-control'\n                        />\n                    </Col>\n                </FormGroup>\n                <FormGroup row>\n                    <Col md = {{size:10, offset:2}}>\n                    <Button type='submit' color = 'primary'>\n                        Send Feedback\n                    </Button>\n                    </Col>\n                </FormGroup>\n            </Form>\n        </Formik>\n    )\n};\n\nexport default ContactForm;"],"mappings":"AAAA,OAAQA,MAAM,CAAEC,KAAK,CAAEC,GAAG,CAAEC,SAAS,KAAO,YAAY,CACxD,OAAQC,MAAM,CAAEC,KAAK,CAAEC,IAAI,CAAEC,YAAY,KAAO,QAAQ,CACxD,OAASC,mBAAmB,KAAQ,iCAAiC,CAAC,OAAAC,GAAA,IAAAC,IAAA,gCAAAC,IAAA,IAAAC,KAAA,yBACtE,KAAM,CAAAC,WAAW,CAAGA,CAAA,GAAM,CACtB,KAAM,CAAAC,YAAY,CAAGA,CAACC,MAAM,CAAAC,IAAA,GAAkB,IAAhB,CAACC,SAAS,CAAC,CAAAD,IAAA,CACrCE,OAAO,CAACC,GAAG,CAAC,cAAc,CAAEJ,MAAM,CAAC,CACnCG,OAAO,CAACC,GAAG,CAAC,kBAAkB,CAAEC,IAAI,CAACC,SAAS,CAACN,MAAM,CAAC,CAAC,CACvDE,SAAS,CAAC,CAAC,CACf,CAAC,CAED,mBACIP,IAAA,CAACN,MAAM,EACHkB,aAAa,CAAE,CACXC,SAAS,CAAE,EAAE,CACbC,QAAQ,CAAE,EAAE,CACZC,QAAQ,CAAE,EAAE,CACZC,KAAK,CAAE,EAAE,CACTC,KAAK,CAAE,KAAK,CACZC,WAAW,CAAE,UAAU,CACvBC,QAAQ,CAAE,EACd,CAAE,CACFC,QAAQ,CAAEhB,YAAa,CACvBiB,QAAQ,CAAEvB,mBAAoB,CAAAwB,QAAA,cAE9BpB,KAAA,CAACN,IAAI,EAAA0B,QAAA,eACDpB,KAAA,CAACT,SAAS,EAAC8B,GAAG,MAAAD,QAAA,eACVtB,IAAA,CAACT,KAAK,EAACiC,OAAO,CAAC,WAAW,CAACC,EAAE,CAAC,GAAG,CAAAH,QAAA,CAAC,YAElC,